{
  "name": "oauth2-mern-instagram",
  "version": "1.0.0",
  "description": "Instagram Clone using MERN with Google OAuth 2.0",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server",
    "server": "cross-env NODE_ENV=development nodemon server",
    "client": "cross-env NODE_ENV=development && cd client && yarn start",
    "dev": "concurrently \"yarn server\" \"yarn client\" "
  },
